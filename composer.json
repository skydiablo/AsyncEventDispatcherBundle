{
  "name": "skydiablo/async-event-dispatcher-bundle",
  "description": "Handle all your events in an asynchronous way",
  "type": "symfony-bundle",
  "keywords": [
    "event",
    "async"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Volker von Hoesslin",
      "email": "skydiablo@gmx.de"
    }
  ],
  "require": {
    "php": ">=7.0",
    "aws/aws-sdk-php": "^3.19",
    "symfony/event-dispatcher": "^3.1"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-0": {
      "SkyDiablo\\AsyncEventDispatcherBundle\\": ""
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
